                    <td><button class="apply-btn">Apply</button></td>
                </tr>
                <tr>
                    <td>VBL LTD</td>
                    <td>₹ 129 - 136</td>
                    <td>2024-06-03</td>
                    <td>2024-06-05</td>
                    <td>130.15 Cr.</td>
                    <td><span class="status upcoming">Upcoming</span></td>
                    <td><button class="apply-btn">Apply</button></td>
                </tr>
                <tr>
                    <td>Tata Motor</td>
                    <td>₹ 129 - 136</td>
                    <td>2024-06-03</td>
                    <td>2024-06-05</td>
                    <td>130.15 Cr.</td>
                    <td><span class="status new-listed">New Listed</span></td>
                    <td><button class="apply-btn">Apply</button></td>
                </tr>
            </tbody>
        </table>
    </div>
